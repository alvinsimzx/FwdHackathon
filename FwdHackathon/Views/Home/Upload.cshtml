<form action="@Url.Action("Upload", "Home")" method="Post" enctype="multipart/form-data">
  <div class="upload-title">
    <h2>Upload</h2>
    <div>
      <label for="fileupload" class="btn btn-primary">
        Upload file
      </label>
      <button class="btn btn-success" onclick="onClick()">Confirm</button>
      <input id="fileupload" type="file" name="files" accept=".csv">
    </div>
  </div>
  <div class="upload-csv">
    <svg class="upload-svg" height="500" width="1100"></svg>
    <h2 class="upload-text">No data selected</h2>
  </div>
</form>


@section Scripts {
  <script src="~/js/upload.js?v=13"></script>
}